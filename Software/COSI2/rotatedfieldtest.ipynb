    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "fig = plt.figure()\n",
    "ax = fig.add_axes(111) \n",
    "x = np.linspace(1,3,10)\n",
    "y = np.linspace(-1,1,10)\n",
    "ax.plot(x,y,'x')\n",
    "\n",
    "X,Y = np.meshgrid(x,y)\n",
    "\n",
    "print(Y)"
   ]
